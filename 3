#include <iostream>
#include <math.h>
 
using namespace std;
 
float RingS(float R1, float R2) {
    float S1, S2, S;
    const float p = 3.14;
    S1 = p * pow(R1, 2);
    S2 = p * pow(R2, 2);
    S = S1 - S2;
    return S;
}
 
int main()
{
    float R1, R2, S;
    for (int i = 1; i <= 3; i++) {
        cout << "Enter the numbers \n";
        cin >> R1 >> R2;
        S = RingS(R1, R2);
        cout << S << " \n"
    }
}
